package com.skillstorm.review;

/**
 * Reusing state and behavior in subclasses
 * 
 * @author PWalsh
 *
 */
public class Inheritance {

}

class Person {
	int energy = 100;
	public void nap() {
		System.out.println("ZzzZz");
	}
}

class Programmer extends Person {
	public void code() {
		// code
		energy--;
		nap();
	}
}