package com.skillstorm.oop;

/**
 * Definition of an object. 
 * 
 * State: attributes, fields, variables
 * Behavior: methods, functions
 * 
 * @author PWalsh
 *
 */
public class Animal {

	/*
	 * Primitive Data Types
	 */
	// -2b -> +2b
	int legs;
	
	double weight; 
	float height; 
	
	long cells;
	// 32,000
	short age; 
	
	boolean alive; 
	char gender = 'A';
	byte bit;
	
	// String class
	//String fname = new String("Dan");
	String fname = "Dan";
	String lname = "Dan";
	
	public static void main(String[] args) {
		Animal dan = new Animal();
		System.out.println(dan.fname == dan.lname); // false
		System.out.println(dan.fname.equals(dan.lname)); // true
	}
	
}







