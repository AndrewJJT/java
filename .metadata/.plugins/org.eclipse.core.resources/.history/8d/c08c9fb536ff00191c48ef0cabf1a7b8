package com.skillstorm.oop;

public class Abstraction {

	public static void main(String[] args) {
		//Animal a = new Animal();
		
		// Covariant
		Animal dan = new Dog();
		Cat c = new Cat();
		
		// Virtual Method Invocation
		dan.speak();
		dan.talk();
		
		Math.pow(2, 5);
		System.out.println(dan.count);
		
	}
	
	public void pet(Animal a) {
		// pet
	}
	
}
