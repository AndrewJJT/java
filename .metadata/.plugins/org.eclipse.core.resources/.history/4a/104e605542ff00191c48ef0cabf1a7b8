package com.skillstorm.review;

public class Polymorphism {

}

class Animal {
	public void speak() {
		System.out.println("Yo");
	}
}

class Dog extends Animal {
	public void speak() {
		System.out.println("Bark");
	}
}

class Cat extends Animal {
	public void speak() {
		System.out.println("Meow");
	}
}