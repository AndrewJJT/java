package com.skillstorm.oop;

/**
 * Definition of an object. 
 * 
 * State: attributes, fields, variables
 * Behavior: methods, functions
 * 
 * @author PWalsh
 *
 */
public class Animal {

	/*
	 * Primitive Data Types
	 */
	// -2b -> +2b
	int legs;
	
	double weight; 
	float height; 
	
	long cells;
	// 32,000
	short age; 
	
	boolean alive; 
	char gender = 'A';
	byte bit;
	
	
	
}
